                    <bsvc:Exclude_Region_Hierarchies>true</bsvc:Exclude_Region_Hierarchies>
                    <bsvc:Exclude_Supervisory_Organizations>true</bsvc:Exclude_Supervisory_Organizations>
                    <bsvc:Exclude_Teams>true</bsvc:Exclude_Teams>
